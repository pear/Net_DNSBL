<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>Net_DNSBL</name>
  <summary>DNSBL Checker</summary>
  <description>Checks if a given Host or URL is listed on an DNSBL or SURBL</description>
  <maintainers>
    <maintainer>
      <user>nohn</user>
      <name>Sebastian Nohn</name>
      <email>sebastian@nohn.net</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>ammarmi</user>
      <name>Ammar Ibrahim</name>
      <email>ammar@gnuix.com</email>
      <role>developer</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1</version>
    <date>2004-11-14</date>
    <license>PHP License</license>
    <state>devel</state>
    <notes>* Now using single quotes instead of double quotes</notes>
    <deps>
      <dep type="pkg" rel="ge" version="1.5.4">Cache</dep>
      <dep type="pkg" rel="ge" version="1.1">Net_CheckIP</dep>
      <dep type="pkg" rel="ge" version="1.2.3">HTTP_Request</dep>
      <dep type="php" rel="ge" version="4.3.6"/>
    </deps>
   <filelist>
   <dir name="/" baseinstalldir="Net">
    <file role="php" name="DNSBL.php" />
    <dir name="DNSBL" role="php">
      <file role="php" name="SURBL.php" />
    </dir>
    <dir name="Tests" role="test">
     <file name="testNetDNSBL.php" />
     <file name="testNetDNSBLSURBL.php" />
    </dir>
   </dir>
  </filelist>
  </release>
